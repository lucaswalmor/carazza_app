{
  "expo": {
    "name": "Carazza",
    "slug": "Carazza",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "myapp",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAvrsH2y7aIRm6eJ-j20J5_LdpIuj_TAmg"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "myapp",
            "CFBundleURLSchemes": [
              "myapp"
            ]
          }
        ],
        "UIBackgroundModes": ["remote-notification"],
        "NSLocationWhenInUseUsageDescription": "Nós precisamos da sua localização para mostrar e gravar a rota.",
        "NSLocationAlwaysUsageDescription": "Nós precisamos da sua localização mesmo quando o aplicativo está em segundo plano."
      },
      "bundleIdentifier": "com.lucassteinbach.Carazza"
    },
    "android": {
      "useNextNotificationsApi": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAPOMVj_GA0krJ0XYq4Ui0CxiB7CGZ5kmQ"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lucassteinbach.Carazza",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "myapp",
              "host": "checkout",
              "pathPrefix": "/success"
            },
            {
              "scheme": "myapp",
              "host": "checkout",
              "pathPrefix": "/failure"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "61721ec3-c518-4701-8453-7bd69acf2c94"
      }
    }
  }
}